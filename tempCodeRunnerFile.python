
from tqdm import tqdm
import time
  
  
for i in tqdm (range (101), 
               desc="Loadingâ€¦", 
               ascii=False, ncols=75):
    time.sleep(0.01)
      
print("Complete.")

n = 10
while 0 < n:
    # add some spaces after the number to make sure we overwrite the entire last output
    print("\r", n, end="  ")
    n -= 1
    time.sleep(0.3)
print("\rBlastoff!")